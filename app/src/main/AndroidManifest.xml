<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.myapplication">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/nabi_icon_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SpendActivity" />
        <activity android:name=".activity.TrafficActivity" />
        <activity android:name=".activity.WeatherActivity" />
        <activity android:name=".activity.MyPageActivity" />
        <activity android:name=".activity.TrafficItemActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.SignActivity"></activity>
        <activity android:name=".activity.weathercalender" />
        <activity android:name=".activity.BookmarkActivity"/>
        <activity android:name=".activity.SurroundingSearchActivity"/>
        <activity android:name=".activity.SearchActivity"/>
    </application>

</manifest>