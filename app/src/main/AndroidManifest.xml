<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/nabi_icon_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.SpendActivity"></activity>
        <activity android:name=".activity.TrafficActivity"></activity>
        <activity android:name=".activity.WeatherActivity"></activity>
        <activity android:name=".activity.MyPageActivity"></activity>
        <activity android:name=".activity.SpendItemActivity"></activity>
        <activity android:name=".activity.TrafficItemActivity"></activity>

    </application>

</manifest>